<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to t(".logo"), root_path, id: "logo" %>
    <nav>
      <ul>
        <% if logged_in? %>
         <li><%= link_to t(".log_out"), "#" %></li>
        <% else %>
         <li><%= link_to t(".log_in"), login_path %></li>
        <% end %>
        <li><%= link_to t(".home"), root_path %></li>
      </ul>
    </nav>
  </div>
</header>
